<div id="labels">
<h4>Labels</h4>
<ul class="nav nav-pills nav-stacked">
<% @labels.each do |label| %>
  <li>
      <%= link_to label.name, {controller: 'plans', action: 'index', label: label.id}, method: 'get' %>
  </li>
<% end %>
</ul>
<a href="/labels/manage" id="manage-labels" class="btn">Manage Labels</a>
</div>
